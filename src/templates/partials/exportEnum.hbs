{{#ifdef description deprecated}}
/**
{{#if description}}
 * {{{escapeComment description}}}
{{/if}}
{{#if deprecated}}
 * @deprecated
{{/if}}
 */
{{/ifdef}}
export enum {{{name}}} {
	{{#each enum}}
	{{#if description}}
	/**
	 * {{{escapeComment description}}}
	 */
	{{/if}}
	{{#containsSpaces name}}
	'{{{name}}}' = {{{value}}},
	{{else}}
	{{{name}}} = {{{value}}},
	{{/containsSpaces}}
	{{/each}}
}

/**
 * Возвращает описание для элемента enum
{{#if description}}
 * {{{escapeComment description}}}
{{/if}}
 * @param { {{~name~}} } val Значение из {{{name}}}
 * @return {string} описание для значения
{{#if deprecated}}
 * @deprecated
{{/if}}
 */
export const descr{{{name}}} = (val: {{{name}}}):string => {
  switch (val) {
		{{#each enum}}
			{{#if description}}
				case {{{value}}}:
					return '{{{description}}}'
			{{/if}}
		{{/each}}
			default:
				return ''
	}
}
