{{>header}}

{{#if @root.exportClient}}
export { {{{clientName}}} } from './{{{clientName}}}';

{{/if}}
{{#if @root.exportCore}}
export { ApiError } from './core/ApiError';
{{#if @root.exportClient}}
export { BaseHttpRequest } from './core/BaseHttpRequest';
{{/if}}
export { CancelablePromise, CancelError } from './core/CancelablePromise';
export { OpenAPI } from './core/OpenAPI';
export type { OpenAPIConfig } from './core/OpenAPI';
{{/if}}
{{#if @root.exportModels}}
{{#if models}}

{{#each models}}
{{#if @root.useUnionTypes}}
export type { {{{name}}} } from './models/{{{name}}}';
{{else if enum}}
export { {{{name}}}, descr{{{name}}} } from './models/{{{name}}}';
{{else if enums}}
export { {{{name}}} } from './models/{{{name}}}';
{{else}}
export type { {{{name}}} } from './models/{{{name}}}';
{{/if}}
{{/each}}

// Nest Dto for Implements
{{#each models}}
{{#if @root.useUnionTypes}}
export type { {{{name}}}Nest } from './dto/{{{name}}}Nest';
{{else if enum}}
{{!-- export { {{{name}}}Nest } from './dto/{{{name}}}Nest'; --}}
{{else if enums}}
export { {{{name}}}Nest } from './dto/{{{name}}}Nest';
{{else}}
export type { {{{name}}}Nest } from './dto/{{{name}}}Nest';
{{/if}}
{{/each}}

{{/if}}
{{/if}}
{{#if @root.exportSchemas}}
{{#if models}}

{{#each models}}
export { ${{{name}}} } from './schemas/${{{name}}}';
{{/each}}
{{/if}}
{{/if}}
{{#if @root.exportServices}}
{{#if services}}

{{#each services}}
export { {{{name}}}{{{@root.postfix}}} } from './services/{{{name}}}{{{@root.postfix}}}';
{{/each}}
{{/if}}
{{/if}}
